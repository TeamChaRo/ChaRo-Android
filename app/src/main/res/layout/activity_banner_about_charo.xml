                            android:layout_marginTop="6dp"
                            app:layout_constraintStart_toStartOf="parent"
                            android:fontFamily="@font/noto_sans_cjkkr_medium"
                            android:includeFontPadding="false"
                            android:textStyle="normal"
                            android:textSize="14sp"
