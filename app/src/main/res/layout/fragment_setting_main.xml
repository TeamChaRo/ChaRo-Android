<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".presentation.ui.setting.SettingMainFragment">


        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/text_access"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/access"
                    android:textSize="12sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:textColor="@color/gray5_sub"
                    android:includeFontPadding="false"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/text_setting_alarm"
                    android:layout_marginTop="22dp"
                    android:layout_marginStart="20dp" />

                <TextView
                    android:id="@+id/text_setting_alarm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/main_charo_notice"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:textColor="@color/black_sub"
                    android:includeFontPadding="false"
                    app:layout_constraintTop_toBottomOf="@+id/text_access"
                    app:layout_constraintStart_toStartOf="@+id/text_access"
                    android:layout_marginTop="24dp"/>

                <Switch
                    android:id="@+id/switch_alarm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:track="@drawable/selector_mypage_switch"
                    android:thumb="@drawable/mypage_switch_thumb"
                    app:layout_constraintTop_toTopOf="@+id/text_setting_alarm"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="20dp"
                    tools:ignore="UseSwitchCompatOrMaterialXml" />

                <TextView
                    android:id="@+id/text_setting_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/image"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_alarm"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_alarm"
                    android:layout_marginTop="17dp"/>

                <Switch
                    android:id="@+id/switch_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:track="@drawable/selector_mypage_switch"
                    android:thumb="@drawable/mypage_switch_thumb"
                    app:layout_constraintTop_toTopOf="@+id/text_setting_image"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="20dp"
                    tools:ignore="UseSwitchCompatOrMaterialXml"/>

                <ImageView
                    android:id="@+id/img_access_line"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_line"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_image"
                    android:layout_marginTop="21dp"/>

                <TextView
                    android:id="@+id/text_setting_marketing_agree"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/marketing_agree"
                    android:includeFontPadding="false"
                    android:textSize="12sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:textColor="@color/gray5_sub"
                    app:layout_constraintTop_toBottomOf="@+id/img_access_line"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_image"
                    android:layout_marginTop="22dp"/>

                <TextView
                    android:id="@+id/text_setting_email_agree"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/email_agree"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_marketing_agree"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_marketing_agree"
                    android:layout_marginTop="25dp"/>

                <Switch
                    android:id="@+id/switch_email_agree"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:track="@drawable/selector_mypage_switch"
                    android:thumb="@drawable/mypage_switch_thumb"
                    app:layout_constraintTop_toTopOf="@+id/text_setting_email_agree"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="20dp"
                    tools:ignore="UseSwitchCompatOrMaterialXml"/>

                <ImageView
                    android:id="@+id/img_agree_line"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_line"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_email_agree"
                    android:layout_marginTop="17dp"/>

                <TextView
                    android:id="@+id/text_setting_account"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/account"
                    android:includeFontPadding="false"
                    android:textSize="12sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:textColor="@color/gray5_sub"
                    app:layout_constraintTop_toBottomOf="@+id/img_agree_line"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_email_agree"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_profile_update"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_update"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    android:clickable="true"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_account"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_account"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_password_update"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/password_update"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_profile_update"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_profile_update"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/email"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_password_update"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_password_update"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_user_email"
                    android:layout_width="0dp"
                    android:layout_height="16dp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/switch_email_agree"
                    app:layout_constraintTop_toTopOf="@+id/text_setting_email" />

                <ImageView
                    android:id="@+id/img_setting_account"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_line"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_email"
                    android:layout_marginTop="17dp"/>

                <TextView
                    android:id="@+id/text_setting_information"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/information"
                    android:includeFontPadding="false"
                    android:textSize="12sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:textColor="@color/gray5_sub"
                    app:layout_constraintTop_toBottomOf="@+id/img_setting_account"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_email"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_notice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/notice"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_information"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_information"
                    android:layout_marginTop="18dp"/>

                <ImageView
                    android:id="@+id/img_setting_information"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_line"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_notice"
                    android:layout_marginTop="17dp"/>
                <TextView
                    android:id="@+id/text_setting_cs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs"
                    android:includeFontPadding="false"
                    android:textSize="12sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:textColor="@color/gray5_sub"
                    app:layout_constraintTop_toBottomOf="@+id/img_setting_information"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_notice"
                    android:layout_marginTop="20dp"/>

                <TextView
                    android:id="@+id/text_setting_cs_query"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cs_query"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_cs"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_cs"
                    android:layout_marginTop="18dp"/>

                <ImageView
                    android:id="@+id/img_setting_cs"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_line"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_cs_query"
                    android:layout_marginTop="17dp"/>

                <TextView
                    android:id="@+id/text_setting_terms"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/terms"
                    android:includeFontPadding="false"
                    android:textSize="12sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:textColor="@color/gray5_sub"
                    app:layout_constraintTop_toBottomOf="@+id/img_setting_cs"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_cs_query"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_privacy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/privacy"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_terms"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_terms"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_service"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/service"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_privacy"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_privacy"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_open_source"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/open_source"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/black_sub"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_service"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_service"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_version"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/version"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/unchecked_box"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_open_source"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_open_source"
                    android:layout_marginTop="18dp"/>


                <TextView
                    android:id="@+id/text_setting_version_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/version_num"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/unchecked_box"
                    app:layout_constraintTop_toTopOf="@+id/text_setting_version"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="20dp"/>

                <TextView
                    android:id="@+id/text_setting_logout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/logout"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/blue_main"
                    android:clickable="true"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_version"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_version"
                    android:layout_marginTop="18dp"/>

                <TextView
                    android:id="@+id/text_setting_user_out"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/user_out"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_cjkkr_regular"
                    android:includeFontPadding="false"
                    android:textColor="@color/red_sign_up"
                    app:layout_constraintTop_toBottomOf="@+id/text_setting_logout"
                    app:layout_constraintStart_toStartOf="@+id/text_setting_logout"
                    android:layout_marginTop="18dp"/>


            </androidx.constraintlayout.widget.ConstraintLayout>





        </androidx.core.widget.NestedScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>