<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.ui.home.BannerAboutCharoActivity">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nscv_banner_about_charo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="34dp"
            >
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar_banner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                android:minHeight="78dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/txt_banner_about_charo"
                    android:textAppearance="@style/HeaderH4"
                    android:layout_gravity="center"
                    />
            </androidx.appcompat.widget.Toolbar>

            <ImageView
                android:id="@+id/iv_main_about_charo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/toolbar_banner"
                android:background="@drawable/banner_img_four_detail"
                android:contentDescription="@string/img_description"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="@+id/iv_main_about_charo"
                app:layout_constraintBottom_toBottomOf="@+id/iv_main_about_charo"
                app:layout_constraintStart_toStartOf="@+id/iv_main_about_charo"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="#3a37384f">
            </View>

            <TextView
                android:id="@+id/tv_sl_about_charo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@+id/iv_main_about_charo"
                app:layout_constraintBottom_toBottomOf="@+id/iv_main_about_charo"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintVertical_bias="0.25"
                android:layout_marginEnd="20dp"
                android:fontFamily="@font/notosanscjkkr_regular"
                android:includeFontPadding="false"
                android:textStyle="bold"
                android:textSize="19sp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="5.7sp"
                android:gravity="end"
                android:text="@string/txt_banner_sl_about_charo"/>

            <ImageView
                android:id="@+id/iv_about_charo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="148dp"
                android:minHeight="51dp"
                app:layout_constraintTop_toBottomOf="@+id/tv_sl_about_charo"
                app:layout_constraintEnd_toEndOf="@+id/tv_sl_about_charo"
                android:background="@drawable/banner_charo"
                android:contentDescription="@string/img_description"
                />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_banner_start_charo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/iv_main_about_charo"
                android:layout_marginTop="33dp"
                android:layout_marginHorizontal="8dp"
                >
                <ImageView
                    android:id="@+id/iv_road_start_charo"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/road_main_animation"
                    android:contentDescription="@string/img_description"
                    />

                <ImageView
                    android:id="@+id/iv_car_start_charo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="11dp"
                    android:layout_marginStart="7dp"
                    android:background="@drawable/car_main_animation_blue"
                    android:contentDescription="@string/img_description"
                    />

                <TextView
                    android:id="@+id/tv_banner_title_start_charo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/notosanscjkkr_regular"
                    android:includeFontPadding="false"
                    android:textStyle="bold"
                    android:textSize="17sp"
                    android:textColor="@color/blue_main_0f6fff"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginEnd="19dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginBottom="16dp"
                    android:text="@string/txt_banner_charo_start"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/iv_road_start_charo"
                    android:layout_marginTop="8dp"
                    android:layout_marginHorizontal="13dp"
                    android:fontFamily="@font/noto_sans_cjkkr_medium"
                    android:textStyle="normal"
                    android:textSize="14sp"
                    android:textColor="#495057"
                    android:lineSpacingExtra="8.4sp"
                    android:text="@string/txt_banner_charo_start_context"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_banner_about_charo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/cl_banner_start_charo"
                android:layout_marginTop="44dp"
                android:layout_marginHorizontal="8dp"
                >
                <ImageView
                    android:id="@+id/iv_road_about_charo"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:background="@drawable/road_main_animation"
                    android:contentDescription="@string/img_description"
                    />

                <ImageView
                    android:id="@+id/iv_car_about_charo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/tv_banner_title_about_charo"
                    app:layout_constraintHorizontal_bias="0.4"
                    android:layout_marginTop="11dp"
                    android:background="@drawable/car_main_animation_blue"
                    android:contentDescription="@string/img_description"
                    />

                <TextView
                    android:id="@+id/tv_banner_title_about_charo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/notosanscjkkr_regular"
                    android:includeFontPadding="false"
                    android:textStyle="bold"
                    android:textSize="17sp"
                    android:textColor="@color/blue_main_0f6fff"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginEnd="19dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginBottom="16dp"
                    android:text="@string/txt_banner_title_charo_for_you"/>

                <androidx.constraintlayout.widget.ConstraintLayout
