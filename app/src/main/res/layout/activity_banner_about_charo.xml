                            app:layout_constraintTop_toBottomOf="@+id/tv_banner_team_charo_plan"
                            android:layout_marginTop="6dp"
                            android:layout_marginStart="22dp"
                            app:layout_constraintStart_toEndOf="@+id/tv_plan_heejin"
                            android:fontFamily="@font/noto_sans_cjkkr_medium"
                            android:includeFontPadding="false"
                            android:textStyle="normal"
                            android:textSize="14sp"
                            android:textColor="#495057"
                            android:lineSpacingExtra="8.4sp"
                            android:text="@string/dabean"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/cl_banner_team_charo_design"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        >
                        <TextView
